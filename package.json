{
  "name": "cyberup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "next dev --turbopack",
    "dev:server": "node server.js",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "prebuild": "npm run format && npm run knip",
    "build": "next build --turbopack",
    "start:client": "next start",
    "start:server": "NODE_ENV=production node server.js",
    "start:all": "concurrently \"npm:start:server\" \"npm:start:client\"",
    "lint": "eslint",
    "knip": "knip",
    "format": "prettier --write .",
    "update": "npm-check-updates -u && npm install",
    "reinstall": "rm -rf .next node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "npm-check-updates": "^19.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.0",
    "knip": "^5.66.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.15",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
